<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saaibi.parcial">

    <application
        android:name=".Controller.ApplicationController"
        android:allowBackup="true"
        android:icon="@mipmap/copagrande"
        android:label="@string/title_activity_admin"
        android:roundIcon="@mipmap/copagrande"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Views.LoginActivity"
            android:label="@string/title_activity_admin"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Views.AdminActivity"
            android:label="@string/title_activity_admin"
            android:theme="@style/AppTheme.NoActionBar" />


        <activity
            android:name=".Views.EventActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Views.EventDetailActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>